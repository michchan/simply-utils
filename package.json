{
  "name": "simply-utils",
  "version": "0.50.1",
  "description": "Simple and generic JavaScript utils for common use cases, mainly developed and refractored from frontend projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clearbuildcache": "rm -rf dist",
    "deploy": "npm run index && npm run build && npm run tag && git push && npm publish",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint 'src/**/*.{js,jsx,ts,tsx}'",
    "index": "node scripts/generateExports && node scripts/generateIndex && git add . && git commit -m \"Update index files\"",
    "tag": "git tag $(node -pe 'require(\"./package.json\").version')",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc --watch --noemit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michchan/simply-utils.git"
  },
  "keywords": [
    "node",
    "npm",
    "javascript",
    "utils",
    "utils-library",
    "utils-lib",
    "utilities",
    "web",
    "frontend"
  ],
  "author": "Michael Chan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michchan/simply-utils/issues"
  },
  "homepage": "https://github.com/michchan/simply-utils#readme",
  "devDependencies": {
    "@michchan/eslint-config": "1.6.0",
    "@types/aws-lambda": "^8.10.63",
    "@types/history": "^4.7.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^14.14.2",
    "@types/puppeteer": "^3.0.1",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.9.53",
    "@types/styled-components": "^5.1.4",
    "@types/superagent": "^4.1.9",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.777.0",
    "chrome-aws-lambda": "5.3.1",
    "eslint": "^7.11.0",
    "file-loader": "^6.0.0",
    "puppeteer": "5.3.0",
    "puppeteer-core": "5.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^5.2.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.3",
    "webpack": "5.2.0"
  },
  "dependencies": {
    "eastasianwidth": "0.2.0",
    "history": "^4.10.1",
    "lodash": "^4.17.19",
    "qs": "^6.9.3",
    "react-device-detect": "^1.14.0"
  },
  "peerDependencies": {
    "chrome-aws-lambda": "5.3.x",
    "file-loader": "6.x",
    "puppeteer-core": "5.3.x",
    "puppeteer": "5.3.x",
    "styled-components": ">= 5.0.0"
  }
}
