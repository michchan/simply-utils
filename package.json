{
  "name": "simply-utils",
  "version": "0.54.0",
  "description": "Simple and generic JavaScript utils for common use cases, mainly developed and refractored from frontend projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clearbuildcache": "rm -rf dist",
    "deploy": "npm run index && npm run build && npm run tag && git push && npm publish",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint 'src/**/*.{js,jsx,ts,tsx}'",
    "index": "ts-node scripts/generateExports && ts-node scripts/generateIndex",
    "index:commit": "npm run index && git add . && git commit -m \"Update index files\"",
    "tag": "git tag $(node -pe 'require(\"./package.json\").version')",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc --watch --noemit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michchan/simply-utils.git"
  },
  "keywords": [
    "node",
    "npm",
    "javascript",
    "utils",
    "utils-library",
    "utils-lib",
    "utilities",
    "web",
    "frontend"
  ],
  "author": "Michael Chan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michchan/simply-utils/issues"
  },
  "homepage": "https://github.com/michchan/simply-utils#readme",
  "devDependencies": {
    "@michchan/eslint-config": "1.7.0",
    "@types/aws-lambda": "^8.10.70",
    "@types/history": "^4.7.8",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.2",
    "@types/puppeteer": "^5.4.2",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/superagent": "^4.1.10",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.824.0",
    "chrome-aws-lambda": "5.3.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "file-loader": "^6.0.0",
    "intl": "^1.2.5",
    "puppeteer": "5.5.0",
    "puppeteer-core": "5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "relative-time-format": "^1.0.5",
    "styled-components": "^5.2.1",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "5.12.2"
  },
  "dependencies": {
    "eastasianwidth": "0.2.0",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "qs": "^6.9.4",
    "react-device-detect": "^1.15.0"
  },
  "peerDependencies": {
    "chrome-aws-lambda": "5.5.x",
    "file-loader": "6.x",
    "puppeteer-core": "5.5.x",
    "puppeteer": "5.5.x",
    "styled-components": ">= 5.0.0",
    "dotenv": "*",
    "intl": "1.x",
    "relative-time-format": "1.x"
  }
}
