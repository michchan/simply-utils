{
  "name": "simply-utils",
  "version": "0.65.0",
  "description": "Simple and generic JavaScript utils for common use cases, mainly developed and refractored from frontend projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16.0.0",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "scripts/build",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "yarn run eslint -- --fix",
    "stylelint": "stylelint 'src/**/*.{js,jsx,ts,tsx}'",
    "index": "ts-node scripts/generateExports && ts-node scripts/generateIndex",
    "index:commit": "yarn run index && git add . && git commit -m \"Update index files\"",
    "tag": "git tag $(node -pe 'require(\"./package.json\").version')",
    "test": "echo \"Error: no test specified\" && exit 0",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch --noemit",
    "generate-docs-tags": "ts-node scripts/generateJSDocsTags",
    "generate-docs": "jsdoc -c jsdoc.json",
    "build-docs": "rm -rf docs && yarn run generate-docs-tags && yarn run generate-docs",
    "serve-docs": "serve docs",
    "prepare": "husky install",
    "release": "HUSKY=0 standard-version --ignore-path .gitignore --commit-hook false",
    "git-check-unclean": "sh scripts/git-check-unclean.sh",
    "distribute": "cd dist && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michchan/simply-utils.git"
  },
  "keywords": [
    "node",
    "npm",
    "yarn",
    "javascript",
    "utils",
    "utils-library",
    "utils-lib",
    "utilities",
    "web",
    "frontend"
  ],
  "author": "Michael Chan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michchan/simply-utils/issues"
  },
  "homepage": "https://github.com/michchan/simply-utils#readme",
  "peerDependencies": {
    "chrome-aws-lambda": "6.x",
    "dotenv": "*",
    "file-loader": "6.x",
    "intl": "1.x",
    "node-fetch": "2.x",
    "puppeteer": "6.x",
    "puppeteer-core": "6.x",
    "react": "17.x",
    "react-dom": "17.x",
    "relative-time-format": "1.x",
    "styled-components": ">= 5.0.0"
  },
  "peerDependenciesMeta": {
    "chrome-aws-lambda": {
      "optional": true
    },
    "dotenv": {
      "optional": true
    },
    "file-loader": {
      "optional": true
    },
    "intl": {
      "optional": true
    },
    "node-fetch": {
      "optional": true
    },
    "puppeteer": {
      "optional": true
    },
    "puppeteer-core": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "relative-time-format": {
      "optional": true
    },
    "styled-components": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.10",
    "eastasianwidth": "0.2.0",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "qs": "^6.9.4",
    "react-device-detect": "^1.15.0"
  },
  "devDependencies": {
    "@michchan/eslint-config": "1.10.1",
    "@types/aws-lambda": "^8.10.70",
    "@types/history": "^4.7.8",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.2",
    "@types/puppeteer": "^5.4.5",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/superagent": "^4.1.10",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.824.0",
    "better-docs": "^2.3.2",
    "chrome-aws-lambda": "6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "file-loader": "^6.0.0",
    "husky": "^8.0.0",
    "intl": "^1.2.5",
    "jsdoc": "^3.6.10",
    "node-fetch": "^2.6.7",
    "puppeteer": "6.0.0",
    "puppeteer-core": "6.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "relative-time-format": "^1.0.5",
    "serve": "^12.0.0",
    "standard-version": "^9.3.2",
    "styled-components": "^5.2.1",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "5.12.2"
  }
}
